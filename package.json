{
  "name": "react-redux-webpack2-boilerplate",
  "version": "0.1.1",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build-high": "rm -rf ./build && NODE_ENV=\"production\" SERVER_ENV=\"high\" webpack",
    "build-low": "rm -rf ./build && NODE_ENV=\"production\" SERVER_ENV=\"low\" webpack",
    "build-mid": "rm -rf ./build && NODE_ENV=\"production\" SERVER_ENV=\"mid\" webpack",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-clear": "jest -u",
    "start": "webpack-dashboard -t 'React-Redux Boilerplate' -- webpack-dev-server --colors --no-info",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "build-dev": "rm -rf ./build && NODE_ENV=\"development\" webpack",
    "lint-break-on-errors": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx",
    "lint": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx || true",
    "preview": "NODE_ENV=\"production\" ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'Preview Mode - React-Redux Boilerplate' -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove"
  },
  "devDependencies": {
    "airbnb-js-shims": "^1.2.0",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chunk-manifest-webpack-plugin": "1.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.14.5",
    "enzyme": "^2.9.1",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "mobx-react-devtools": "^4.2.15",
    "mobx-utils": "^3.2.1",
    "nib": "^1.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^1.1.1",
    "prepush": "^3.1.11",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-polyfill": "^6.16.0",
    "es6-promise": "^3.1.2",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "mobx-react-devtools": "^4.2.11",
    "mobx-react-form": "^1.32.2",
    "mobx-react-form-devtools": "^1.7.2",
    "mobx-utils": "^3.2.1",
    "modul-components": "^0.0.9",
    "modul-helpers": "^0.1.9",
    "node-uuid": "^1.4.8",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.5.9",
    "q": "^1.4.1",
    "react": "^15.6.1",
    "react-click-outside": "^2.3.1",
    "react-dom": "^15.4.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-waypoint": "^7.0.3",
    "validator": "^9.0.0",
    "modul-ui-router": "^0.0.6"
  },
  "description": "Starter boilerplate for React and Redux, using Webpack 2",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/brcportal2/ModulBank.FrontArchitecture2.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack2",
    "boilerplate"
  ],
  "author": "Stanko",
  "bugs": {
    "url": "https://github.com/brcportal2/ModulBank.FrontArchitecture2/issues"
  },
  "homepage": "https://github.com/brcportal2/ModulBank.FrontArchitecture2#readme",
  "prepush": [
    "npm run lint-break-on-errors --silent"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "source/js"
    ]
  }
}
